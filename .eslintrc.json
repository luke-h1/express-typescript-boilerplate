{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "no-console": 0,
      "no-underscore-dangle": "off",
      "no-unused-vars": ["off"],
      "no-use-before-define": ["error", { "variables": false }],
      "no-multi-str": "off",
      "no-unused-expressions": "off",
      "import/extensions": "off",
      "prefer-destructuring": "off",
      "func-names": "off",
      "max-len": "off",
      "no-return-await": "off",
      "import/prefer-default-export": "off"
    }
  }